<!doctype html>
<html>
<head>
  <meta charset="utf-8">
	<!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge"><![endif]-->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
	<title><%= current_page.data.title || "DevConf 2018" %></title>

  <meta name="description" content="DevConf 2018 Website">
	<meta name="keywords" content="devconf, software, developers, conference">
	<meta name="author" content="DevConf">

  <meta name="twitter:card" content="summary_large_image" />
	<meta name="twitter:site" content="@devconfpl" />
	<meta name="twitter:creator" content="@devconfpl" />
	<meta prefix="og: http://ogp.me/ns#" property="og:url" content="http://devconf.pl" />
	<meta prefix="og: http://ogp.me/ns#" property="og:type" content="website" />
	<meta prefix="og: http://ogp.me/ns#" property="og:title" content="DevConf 2018" />
	<meta prefix="og: http://ogp.me/ns#" property="og:description" content="A technology agnostic software development conference with high-level inspirational talks, deep dives into certain subjects, live coding sessions and plenty of networking time." />
	<meta prefix="og: http://ogp.me/ns#" property="og:image" content="http://devconf.pl/images/social_media_header.png" />
	<meta property="fb:app_id" content="230234777452193" />

  <link rel="shortcut icon" href="/images/favicon.ico">
	<link rel="apple-touch-icon" href="/images/apple-touch-icon.png">
	<link rel="apple-touch-icon" sizes="72x72" href="/images/apple-touch-icon-72x72.png">
	<link rel="apple-touch-icon" sizes="114x114" href="/images/apple-touch-icon-114x114.png">
	<link rel="icon" type="image/png" href="/images/favicon-32x32.png" sizes="32x32">
	<link rel="icon" type="image/png" href="/images/favicon-16x16.png" sizes="16x16">

  <%= stylesheet_link_tag "custom-animations" %>
  <%= stylesheet_link_tag "style" %>
  <%= stylesheet_link_tag "lib/jquery.fancybox.min" %>

  <!--[if lt IE 9]>
    <%= javascript_include_tag "html5shiv" %>
    <%= javascript_include_tag "respond.min" %>
	<![endif]-->

  <style>
		.overlay.overlay-clr[class*="bg"]::before {
				background-image: url('/images/backgrounds/bg-overlay.png');
				background-color: rgba(0, 0, 0, 0.4);
				background-repeat: repeat;
				background-size: 2px 2px;
				background-position: top left;
		}

		.overlay .heading-alt .fa {
				color: #4ecdc4;
		}

		.extra-heading > img {
				width: 60%;
				margin-top: 20px;
		}

		#navigation {
			margin-left: 15px;
		}
	</style>
</head>
<body>
  <div class="preloader-mask">
    <div class="preloader"></div>
  </div>

  <%= yield %>

  <%= partial "footer" %>

  <!--[if lt IE 9]>
    <%= javascript_include_tag "jquery-1.11.3.min" %>
	<![endif]-->
	<!--[if (gte IE 9) | (!IE)]><!-->
    <%= javascript_include_tag "jquery-2.1.4.min" %>
	<!--<![endif]-->

	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAYNDJnxHoWSOqKZIrlpAdXOHHqHdwkDpg&v=3&libraries=places"></script>
  <%= javascript_include_tag "bootstrap.min" %>
  <%= javascript_include_tag "jquery.validate.min" %>
  <%= javascript_include_tag "toastr.min" %>
  <%= javascript_include_tag "jquery.waypoints.min" %>
  <%= javascript_include_tag "jquery.appear" %>
  <%= javascript_include_tag "jquery.plugin" %>
  <%= javascript_include_tag "jquery.countTo" %>
  <%= javascript_include_tag "masonry.pkgd.min" %>
  <%= javascript_include_tag "modal-box" %>
  <%= javascript_include_tag "ventcamp" %>
  <%= javascript_include_tag "jquery.fancybox.min" %>

  <!-- TODO: custom scripts -->

  <script>
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

ga('create', 'UA-100957120-1', 'auto');
ga('send', 'pageview');

	</script>
</body>
</html>
